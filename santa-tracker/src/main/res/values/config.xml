<resources>
    <!-- Backoff parameters for API access -->
    <!-- Initital backoff time: 30s -->
    <integer name="backoff_initital" translatable="false">30000</integer>
    <!-- Maximum backoff time: 5m-->
    <integer name="backoff_max" translatable="false">300000</integer>
    <!-- Backoff factor for each retry in percent: 150% -->
    <integer name="backoff_factor" translatable="false">150</integer>

    <!-- StreetView Intent URI with parameters: pano id, heading -->
    <string name="streetview_uri" translatable="false">google.streetview:panoid=%s&amp;cbp=0,%f</string>
    c
    <!-- App Indexing -->
    <!-- Deep link intent filter entries in AndroidManifest.xml must match this prefix -->
    <string name="santa_tracker_deep_link_prefix" translatable="false">http/google.com/santatracker/android</string>
    <!-- deep link component names -->
    <string name="gumball_game_id" translatable="false">gumball</string>
    <string name="jetpack_game_id" translatable="false">jetpack</string>
    <string name="memory_game_id" translatable="false">memory</string>
    <string name="city_quiz_game_id" translatable="false">cityquiz</string>

    <!-- Whether to show debug menu items -->
    <bool name="show_debug_menu_items">true</bool>

</resources>
